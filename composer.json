{
    "name": "fonsecas72/coding-standard",
    "description": "The Symfony2 coding standard",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomáš Votruba",
            "email": "tomas.vot@gmail.com",
            "homepage": "http://tomasvotruba.cz",
